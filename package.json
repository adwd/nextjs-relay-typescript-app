{
  "name": "with-relay-modern",
  "version": "1.0.0",
  "description": "Example of Next.js with Relay Modern",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "relay": "yarn run relay-compiler --schema ./schema/schema.graphql --src ./src/ --language typescript --artifactDirectory ./src/lib/__generated__ --customScalars.DateTime=String --watchman false $@",
    "schema": "graphql get-schema -e dev"
  },
  "license": "MIT",
  "dependencies": {
    "graphql": "^15.5.0",
    "next": "latest",
    "react": "^0.0.0-experimental-696e736be",
    "react-dom": "^0.0.0-experimental-696e736be",
    "react-relay": "^0.0.0-experimental-4c4107dd"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-relay": "^7.0.17",
    "babel-plugin-relay": "^10.1.3",
    "graphql-cli": "^4.1.0",
    "relay-compiler": "^10.1.3",
    "relay-compiler-language-typescript": "^13.0.4",
    "typescript": "^4.1.5"
  }
}
